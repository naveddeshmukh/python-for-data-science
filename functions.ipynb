   "source": [
    "def fib(n, cache={}):  # n = 3\n",
    "    # exit condition\n",
    "    if n < 2:\n",
    "        return 1\n",
    "\n",
    "    if n in cache:\n",
    "        return cache[n]\n",
    "\n",
    "    cache[n] = fib(n - 1, cache) + fib(n - 2, cache)\n",
    "\n",
    "    return cache[n]\n",
    "\n",
    "\n",
    "print(fib(5))"
   ]
